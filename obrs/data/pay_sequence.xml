<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="payment_sequence" model="ir.sequence">
            <field name="name">Payment Sequence</field>
            <field name="code">payment.seq</field>
            <field name="prefix">PAY</field>
            <field name="padding">5</field>
        </record>

        <record id="sequence_book_ref" model="ir.sequence">
            <field name="name">book.sequence</field>
            <field name="code">book.sequence</field>
            <field name="active">TRUE</field>
            <field name="prefix">B</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>